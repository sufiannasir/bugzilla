<h1>User Signup</h1>

<%= form_for @user, :url=> signup_process_path do |f| -%> 

<%= f.label :name %>: <%= f.text_field :name %><br/><br/>
<%= f.label :email %>: <%= f.email_field :email %><br/><br/>
<%= f.label :password %>: <%= f.text_field :password %><br/><br/>
<%= f.label :user_type %>: <%= f.text_field :user_type %><br/><br/>
<%= f.submit :signup %> <% end -%>

