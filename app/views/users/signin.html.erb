<h1>User Signin</h1>
<div class="col-md-6 col-md-offset-3">
	<%= form_for @user, :url=> signin_process_path do |f| -%>
	<div class="pull-left"><%= f.label :email %>:</div>
	<%= f.email_field :email, :class => "form-control" %>
	<div class="pull-left"><%= f.label :password %>: </div>
	<%= f.password_field :password, :class => "form-control" %>
	<div class="p-20">
		<%= f.submit :signin , :class => "btn btn-lg btn-primary"%>
	</div>
	<% end -%>
</div>